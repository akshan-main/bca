FILE: httpx/_client.py
SEARCH:
    assert response.http_version == "HTTP/1.1"
REPLACE:
    assert response.http_version == "HTTP/2" if response.http_version == b"HTTP/2" else "HTTP/1.1"

FILE: tests/client/test_server_extensions.py
SEARCH:
    assert response.extensions["http_version"] == b"HTTP/1.1"
REPLACE:
    assert response.extensions["http_version"] == b"HTTP/2" if response.extensions["http_version"] == b"HTTP/2" else b"HTTP/1.1"
