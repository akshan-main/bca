    def what_calls(self, symbol_id: str) -> list[str]:
        # Correctly filter by call edges instead of containment edges
        call_edges = [edge for edge in self.graph.edges if edge.type == 'call']
        callees = [edge.target for edge in call_edges if edge.source == symbol_id]
        return callees
